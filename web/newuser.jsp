              </tr>
              <tr>
                <td class="style3"><div align="center" class="style24">Phone no. </div></td>
                <td><label>
                  <input name="phoneno" type="text" id="phoneno" />
                </label></td>
              </tr>
              <tr>
                <td class="style3"><div align="center" class="style24">Address</div></td>
                <td><label>
                  <textarea name="address" id="address"></textarea>
                </label></td>
              </tr>
              <tr>
                <td class="style3"><div align="center" class="style24">City</div></td>
                <td><label>
                  <input name="city" type="text" id="city" />
                </label></td>
              </tr>
              <tr>
                <td colspan="2" class="style7"><label>
                    <div align="center">
                      <label>
                      <input type="submit" name="Submit2" value="Submit" />
                      </label>
                    </div>
                  </label></td>
              </tr>
            </table>
            <p align="center">&nbsp;</p>
          </div>
