            <table width="519" height="400" border="0">
              <tr>
                <td height="56" colspan="2"><div align="center"><span class="style27">New User </span> </div></td>
              </tr>
              <tr>
                <td width="326"><div align="center" class="style24"><span class="style3">Name</span></div></td>
                <td width="326"><label>
                  <input name="name" type="text" id="name" />
                </label></td>
              </tr>
              <tr>
                <td class="style7"><div align="center" class="style25">Username</div></td>
                <td><input name="username" type="text" id="username" /></td>
              </tr>
              <tr>
                <td class="style3"><div align="center" class="style24">Password</div></td>
                <td><label>
                  <input name="password" type="password" id="password" />
                </label></td>
              </tr>
              <tr>
                <td class="style3"><div align="center" class="style24">Email ID </div></td>
                <td><label>
                  <input name="emailid" type="text" id="emailid" />
